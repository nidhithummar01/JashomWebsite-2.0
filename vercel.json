{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "redirects": [
      {
        "source": "/:path((?!.*\\.).*[^/]$)",
        "destination": "/:path/",
        "permanent": true
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ]
  }